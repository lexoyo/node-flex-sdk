{
  "name": "flex-sdk2",
  "description": "This is a fork, which will be contributed after [this issue](https://github.com/JamesMGreene/node-flex-sdk/pull/8) is solved. A Node.js module to download and \"install\" the latest open source Apache/Adobe Flex SDK.",
  "version": "4.6.1",
  "flexSdk": {
    "url": "http://fpdownload.adobe.com/pub/flex/sdk/builds/flex4.6/flex_sdk_4.6.0.23201_mpl.zip"
  },
  "homepage": "https://github.com/lexoyo/node-flex-sdk",
  "author": {
    "name": "James M. Greene",
    "email": "james.m.greene@gmail.com",
    "url": "http://jamesgreene.net/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/lexoyo/node-flex-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/lexoyo/node-flex-sdk/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/lexoyo/node-flex-sdk/blob/master/LICENSE-MIT"
    }
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "main": "lib/flex",
  "bin": {
    "mxmlc": "./bin/mxmlc",
    "compc": "./bin/compc"
  },
  "scripts": {
    "prepublish": "node prepublish.js; if [ \"$npm_package_config_npm_cmd_ran\" == \"install\" ]; then npm run-script postinstall; fi; npm config delete $npm_package_name:npm_cmd_ran",
    "preinstall": "npm install playerglobal-latest",
    "install": "npm config set $npm_package_name:npm_cmd_ran install",
    "postinstall": "node install.js",
    "test": "grunt travis --verbose"
  },
  "dependencies": {
    "playerglobal-latest": "^0.1.6",
    "mkdirp": "^0.3.5"
  },
  "devDependencies": {
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-nodeunit": "^0.3.3",
    "grunt": "^0.4.4",
    "download": "^0.1.12",
    "rimraf": "^2.2.6",
    "dos2unix": "^1.1.1",
    "async": "^0.6.2"
  },
  "keywords": [
    "download",
    "install",
    "apache",
    "adobe",
    "flex sdk",
    "open source",
    "compiler",
    "flex",
    "mxml",
    "actionscript",
    "flv",
    "flash",
    "air"
  ]
}
